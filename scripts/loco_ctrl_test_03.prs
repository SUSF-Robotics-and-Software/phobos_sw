# Simple test which drives the motors at 10% forward speed for 5 seconds.

1.0: {
    "type": "MNVR",
    "payload": {
        "mnvr_type": "POINT_TURN",
        "rate_rads": -0.8
    }
};


5.0: {
    "type": "MNVR",
    "payload": {
        "mnvr_type": "POINT_TURN",
        "rate_rads": 0.8
    }
};

10.0: {
    "type": "MNVR",
    "payload": {
        "mnvr_type": "STOP"
    }
};